<body ng-controller="VendedorCtrl">
    <div class="container">
        <div class="page-header">
            <h1>Adicionar Empresa</h1>
        </div>
        <div>
            <div class="main">
                <br />
                <br />
                <div>
                    <a href="#" class="btn btn-primary btn-xs">
                        <span class="glyphicon glyphicon-search"></span> Procurar
                    </a>
                </div>
                <br />
                <form class="form-horizontal">
                    <div class="form-group">
                        <label for="Empresa" class="control-label col-lg-1">Empresa</label>
                        <div class="col-xs-4">
                            <input class="form-control" id="empresa" placeholder="Empresa" type="text">
                        </div>
                        <label for="CNPJ" class="control-label col-lg-1">CNPJ</label>
                        <div class="col-md-3">
                            <input class="form-control" id="CNPJ" placeholder="CNPJ" type="text">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Vendedor" class="control-label col-lg-1">Vendedor</label>
                        <div class="col-xs-4">
                            <input class="form-control" id="vendedor" placeholder="Vendedor" type="text">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Telefone" class="control-label col-lg-1">Telefone</label>
                        <div class="col-md-3">
                            <input class="form-control" id="telefone" placeholder="Telefone" type="text">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Email" class="control-label col-lg-1">Email</label>
                        <div class="col-xs-4">
                            <input class="form-control" id="email" placeholder="Email" type="text">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Compradores" class="control-label col-lg-1">Compradores</label>
                        <div class="col-xs-4 btn-block">
                            <select class="" multiple data-max-options="2">
                                <option>Plain</option>
                                <option>Steamed</option>
                                <option>Toasted</option>
                            </select>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</body>
</html>
